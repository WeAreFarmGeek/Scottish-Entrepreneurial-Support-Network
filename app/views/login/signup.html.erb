<div class="admin_login">
  <h2>Login</h2>
  <%= form_for :user, :url => signup_path do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %>

    <br/>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <br/>

    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>

    <br/>

    <%= f.submit "Register" %>
  <% end %>
</div>
